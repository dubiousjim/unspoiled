mdelete t::tree old = catch handle (fst $ pop old t) /* child, _, _ */ with
    handle abort = t; // throw out_of_bounds;
    handle e = throw e;
    // If elements are discernable but still EQ, note:
    // We delete the element added most recently, which we can do efficiently.
    // avltrees on the other hand removes the element added first.
    pop old nil = throw abort;
    pop old cur@(bin l x@(xk=>xs) r) = case cmp old xk of
      res::int
        = shrink_left (pop old) cur if res == LT;
        = case xs of
            [x] = if r===nil then triple l 1 x
                  else (shrink_right (\_ -> triple child snag x) (bin l w r) when
                    triple child snag w = pop_left r
                  end);
            x:xs = triple (bin l (xk=>xs) r) 0 x;
          end if res == EQ;
        = shrink_right (pop old) cur if res == GT;
    end;
    pop old cur@(tri l y@(yk=>ys) c z@(zk=>zs) r) = case cmp2 old yk zk of
      res::int
        = shrink_left (pop old) cur if res == LT;
        = case ys of
            [y] = if c===nil then triple (bin l z r) 0 y
                  else (shrink_center (\_ -> triple child snag y) (tri l w c z r) when
                    triple child snag w = pop_left c
                  end);
            y:ys = triple (tri l (yk=>ys) c z r) 0 y;
          end if res == EQ;
        = shrink_center (pop old) cur if res == LT2;
        = case zs of
