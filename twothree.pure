minsert t::tree new = fst $ ins new t with
    ins new nil = pair (bin nil (new=>[new]) nil) true; // bump height at prev level
    ins new (bin l1 x1@(xk=>xs) r1) = case cmp new xk of
      res::int
        = case ins new l1 of
            pair child 0 = pair (bin child x1 r1) false;
            pair (bin l2 x2 r2) 1 = pair (tri l2 x2 r2 x1 r1) false;
          end if res == LT;
        = pair (bin l1 (xk=>new:xs) r1) false if res == EQ;
        = case ins new r1 of
            pair child 0 = pair (bin l1 x1 child) false;
            pair (bin l2 x2 r2) 1 = pair (tri l1 x1 l2 x2 r2) false;
          end if res == GT;
    end;
    ins new (tri l1 y1@(yk=>ys) c1 z1@(zk=>zs) r1) = case cmp2 new yk zk of
      res::int
        = case ins new l1 of
                pair child 0 = pair (tri child y1 c1 z1 r1) false;
                pair child 1 = pair (bin child y1 (bin c1 z1 r1)) true; // bump height at prev level
          end if res == LT;
        = pair (tri l1 (yk=>new:ys) c1 z1 r1) false if res == EQ;
        = case ins new c1 of
                pair child 0 = pair (tri l1 y1 child z1 r1) false;
                pair (bin l2 x2 r2) 1 = pair (bin (bin l1 y1 l2) x2 (bin r2 z1 r1)) true; // bump height at prev level
          end if res == LT2;
        = pair (tri l1 y1 c1 (zk=>new:zs) r1) false if res == EQ2;
        = case ins new r1 of
            pair child 0 = pair (tri l1 y1 c1 z1 child) false;
            pair child 1 = pair (bin (bin l1 y1 c1) z1 child) true; // bump height at prev level
          end if res == GT2;
    end;
end;

