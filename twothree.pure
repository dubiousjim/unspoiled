/* Hashed trees, single value. */

public hinsert hinsertk;

hinsert t::tree new = catch handle (fst $ ins (hash new) new t) with
    handle abort = t;
    handle e = throw e;
    add new ws [] = new:ws; // adding new to front of list is less presure on GC
    add new _ (new:_) = throw abort; // keep existing tree
    add new ws (u:us) = add new ws us;
    ins h::int new nil = pair (bin nil (h=>[new]) nil) true; // bump height at prev level
    ins h::int new (bin l1 x1@(xh::int=>xs) r1) = case cmp h xh of
      res::int
        = case ins h new l1 of
            pair child 0 = pair (bin child x1 r1) false;
            pair (bin l2 x2 r2) 1 = pair (tri l2 x2 r2 x1 r1) false;
          end if res == LT;
        = pair (bin l1 (xh=>add new xs xs) r1) false if res == EQ;
        = case ins h new r1 of
            pair child 0 = pair (bin l1 x1 child) false;
            pair (bin l2 x2 r2) 1 = pair (tri l1 x1 l2 x2 r2) false;
          end if res == GT;
    end;
    ins h::int new (tri l1 y1@(yh::int=>ys) c1 z1@(zh::int=>zs) r1) = case cmp2 h yh zh of
      res::int
        = case ins h new l1 of
                pair child 0 = pair (tri child y1 c1 z1 r1) false;
                pair child 1 = pair (bin child y1 (bin c1 z1 r1)) true; // bump height at prev level
          end if res == LT;
        = pair (tri l1 (yh=>add new ys ys) c1 z1 r1) false if res == EQ;
        = case ins h new c1 of
                pair child 0 = pair (tri l1 y1 child z1 r1) false;
                pair (bin l2 x2 r2) 1 = pair (bin (bin l1 y1 l2) x2 (bin r2 z1 r1)) true; // bump height at prev level
          end if res == LT2;
        = pair (tri l1 y1 c1 (zh=>add new zs zs) r1) false if res == EQ2;
        = case ins h new r1 of
